      <param-name>config</param-name>
      <param-value>/s_config.xml</param-value>
    </init-param>
    <init-param>
      <param-name>monitorPeriod</param-name>
      <param-value>1000</param-value>
    </init-param>
    <init-param>
      <param-name>suffix</param-name>
      <param-value>.s</param-value>
    </init-param>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>sController</servlet-name>
    <url-pattern>*.s</url-pattern>
  </servlet-mapping>
